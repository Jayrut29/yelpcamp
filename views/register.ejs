<% include partials/header %>

<form class="form-signin" action="/register" method="POST">

     <h2 class="form-signin-heading">Registration Page</h2>
     <label for="inputEmail" class="sr-only">Username</label>
    <input id="inputEmail" class="form-control" type="text" name="username" placeholder="username" require autofocus>
     <label for="inputPassword" class="sr-only">Password</label>
    <input id="inputPassword" class="form-control" type="password" name="password" placeholder="password">
    <button class="btn btn-lg btn-primary btn-block" type="submit">Sign Up!</button>
    
</form>

<% include partials/footer %>
